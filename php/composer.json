{
    "name": "jomiel-client-demos/php",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.4 <9.0",
        "docopt/docopt": "^1.0",
        "google/protobuf": "^3.15"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/demo/jomiel.php",
            "src/demo/options.php",
            "src/demo/runner.php",
            "src/demo/util.php"
        ]
    },
    "config": {
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "overtrue/phplint": "^2.3",
        "phpmd/phpmd": "^2.9",
        "phpstan/phpstan": "^0.12.83",
        "vimeo/psalm": "^4.7"
    }
}
