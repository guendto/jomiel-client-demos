{
    "parser": "@babel/eslint-parser",
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"]
    }
}
